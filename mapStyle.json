{
    "version": 8,
    "name": "Raster Tiles",
    "sources": {
        "vic-topo": {
            "attribution" : "Australia VIC Topo Gov Maps",
            "type": "raster",
            "tiles": [
                "https://mapshare.maps.vic.gov.au/pwms/arcgis/rest/services/Basemaps/Mapscape_WebMercator/MapServer/tile/{z}/{y}/{x}"
            ],
            "tileSize": 256,
            "maxZoom":26,
            "minZoom":8
        }
    },
    "layers": [
    {
        "id": "vic-topo",
        "type": "raster",
        "source": "vic-topo",
        "paint": {
            "raster-fade-duration": 100
        },
        "layout": {
            "visibility":"visible"
        }
    }
    ]
}
